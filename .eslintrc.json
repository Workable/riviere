{
  "extends": [
    "workable-node",
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": 2017
  },
  "plugins": [
    "prettier"
  ],
  "rules": {
    "operator-linebreak": "off",
    "security/detect-object-injection": "off",
    "max-len": [
      "error",
      {
        "code": 120, "ignoreUrls": true
      }
    ]
  }
}
